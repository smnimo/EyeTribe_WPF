<Window x:Class="eyetracker.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:eyetracker="clr-namespace:eyetracker"
    mc:Ignorable="d"
    Title="MainWindow" 
    AllowsTransparency="True"
    Background="{x:Null}"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    WindowState="Maximized" Visibility="Visible">
    <Grid x:Name="LayoutRoot">
        <Canvas x:Name="canvas_overlay">
            <Ellipse x:Name="GazePointer" Width="100" Height="100" Fill="Blue" Visibility="Visible"/>
        </Canvas>
    </Grid>
</Window>
